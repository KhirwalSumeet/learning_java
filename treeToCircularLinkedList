/* Node of a doubly linked list */
import java.util.Scanner;
class Node
{
    int data;
    Node left; // Pointer to next node in DLL
    Node right; // Pointer to previous node in DLL
    Node(int d) {
        data = d;
        left = null;
        right = null;
    }
};
class DoublyLinkedList
{
    Node root = null;
    
    public void insertData (int value) {
        if (root == null) {
            Node newdata = new Node(value);
            root = newdata;
        } else {
            Node leaf = root;
            while(leaf != null) {
                
                if (leaf.data < value) {
                    if (leaf.right == null) {
                        Node newdata = new Node(value);
                        leaf.right = newdata;
                        break;
                    } else {
                        leaf = leaf.right;
                        continue;
                    }
                } else {
                    if (leaf.left == null) {
                        Node newdata = new Node(value);
                        leaf.left = newdata;
                        break;
                    } else {
                        leaf = leaf.left;
                        continue;
                    }
                }
            }
        }
    }
    
    public void printTree(Node leaf) {
        if (leaf.left != null)
            printTree(leaf.left);
        System.out.println(leaf.data);
        if (leaf.right != null)
            printTree(leaf.right);
    }
    
    public void toCircularLinkedList(Node leaf, Node circular) {
        if (leaf.left != null)
            toCircularLinkedList(leaf.left, circular);
        if (circular == null) {
            Node newdata = new Node(leaf.data);
            circular = newdata;
        } else {
            Node newdata = new Node(leaf.data);
            leaf.right = newdata;
            newdata.left = leaf;
        }
        if (leaf.right != null)
            toCircularLinkedList(leaf.right, circular);
    }
    
    public static void main(String args[]){
        Scanner scanner = new Scanner(System.in);
        DoublyLinkedList dl = new DoublyLinkedList();
        
        // System.out.println("Enter your username: ");
        // String username = scanner.nextLine();
        
        int isInput = 1;
        while (isInput == 1) {
            dl.insertData(Integer.parseInt(scanner.nextLine()));
            isInput = Integer.parseInt(scanner.nextLine());
        }
        dl.printTree(dl.root);
        dl.toCircularLinkedList(dl.root, null);
    }
}
